{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "sourceMap": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "strict": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "lib": ["esnext"],
    "types": ["node", "mocha"],
    "esModuleInterop": true,
    "outDir": "build"
  }
}
